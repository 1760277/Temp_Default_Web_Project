<%- include('User_Header', {title: 'List Saving Account'}); -%>
<div class="w3ls-section contact">
  <div class="container"> 
    <div class="w3ls-title">
      <h2 class="h3-w3l">Saving Account</h2> 
    </div>
    <div class="contact_wthreerow agileits-w3layouts">
      <div class="center">
        <ul>
            <% listSavingAccount.forEach( function (t) { %>
              <li>
                <%= t.savingAccountNumber %> / <%= t.status %> / <%= t.accountType %> / <%= t.moneySending %> / <%= t.interestRate %> <%= t.closeDate %>
                <a class="btn btn-success" href="/Controller/savingAccount.js/t.id %>/confirm" role="button">Submit</a>
              </li>
            <% }); %>
          </ul>
      </div>
    </div>
    <div class="clearfix"></div>
  </div>
</div>
<!-- //contact --> 
<%- include('User_Footer'); -%> 
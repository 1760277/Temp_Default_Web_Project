<%- include('User_Header', {title: 'My Saving Account'}); -%>
<div class="w3ls-section contact">
  <div class="container"> 
    <div class="w3ls-title">
      <h2 class="h3-w3l">Saving Account</h2> 
    </div>
    <div class="contact_wthreerow agileits-w3layouts">
      <div class="center">
        <ul>
          <% savingAccount.forEach( function (t) { %>
            <li>
              Account Number: <%= t.savingAccountNumber %>, Status: <%= t.status %>, Account Type: <%= t.accountType %>, Money Sending: <%= t.moneySending %>, InterestRate <%= t.interestRate %>, Close Date: <%= t.closeDate %>
              <a class="btn btn-success" href="/savingAccount/<%= t.id %>/close" role="button">Close Account</a>
            </li>
          <% }); %>
        </ul>
      </div>
    </div>
    <div class="clearfix"></div>
  </div>
</div>
<!-- //contact --> 
<%- include('User_Footer'); -%> 